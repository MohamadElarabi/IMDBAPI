{
  "env": {
    "es6": true,
    "node": true,
    "browser": false
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
